{
    "name": "symbol-complete",
    "displayName": "symbol-complete",
    "description": "Auto-replace a list of customizable patterns with specified symbols",
    "version": "0.0.3",
    "publisher": "sporiley",
    "engines": {
        "vscode": "^1.19.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.symbolComplete"
    ],
    "main": "./extension",
    "contributes": {
        "commands": [{
                "command": "extension.symbolComplete",
                "title": "Symbol Complete"
            }
        ],
        "configuration": {
            "type": "object",
            "title": "Symbol Complete Configuration",
            "properties": {
                "symbol-complete.replacements": {
                    "type": "object",
                    "default": {
                        "\\|alpha\\|": "α",
                        "\\|Alpha\\|": "Α",
                        "\\|beta\\|": "β",
                        "\\|Beta\\|": "Β",
                        "\\|gamma\\|": "γ",
                        "\\|Gamma\\|": "Γ",
                        "\\|delta\\|": "δ",
                        "\\|Delta\\|": "Δ",
                        "\\|epsilon\\|": "ε",
                        "\\|Epsilon\\|": "Ε",
                        "\\|zeta\\|": "ζ",
                        "\\|Zeta\\|": "Ζ",
                        "\\|eta\\|": "η",
                        "\\|Eta\\|": "Η",
                        "\\|theta\\|": "θ",
                        "\\|Theta\\|": "Θ",
                        "\\|iota\\|": "ι",
                        "\\|Iota\\|": "Ι",
                        "\\|kappa\\|": "κ",
                        "\\|Kappa\\|": "Κ",
                        "\\|lambda\\|": "λ",
                        "\\|Lambda\\|": "Λ",
                        "\\|mu\\|": "μ",
                        "\\|Mu\\|": "Μ",
                        "\\|nu\\|": "ν",
                        "\\|Nu\\|": "Ν",
                        "\\|xi\\|": "ξ",
                        "\\|Xi\\|": "Ξ",
                        "\\|omicron\\|": "ο",
                        "\\|Omicron\\|": "Ο",
                        "\\|pi\\|": "π",
                        "\\|Pi\\|": "Π",
                        "\\|rho\\|": "ρ",
                        "\\|Rho\\|": "Ρ",
                        "\\|sigma\\|": "σ",
                        "\\|sigma\\*\\|": "ς",
                        "\\|Sigma\\|": "Σ",
                        "\\|tau\\|": "τ",
                        "\\|Tau\\|": "Τ",
                        "\\|upsilon\\|": "υ",
                        "\\|Upsilon\\|": "Υ",
                        "\\|phi\\|": "φ",
                        "\\|Phi\\|": "Φ",
                        "\\|chi\\|": "χ",
                        "\\|Chi\\|": "Χ",
                        "\\|psi\\|": "ψ",
                        "\\|Psi\\|": "Ψ",
                        "\\|omega\\|": "ω",
                        "\\|Omega\\|": "Ω"
                    }
                }
            }
        }
    },
    "scripts": {
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.6",
        "eslint": "^4.6.1",
        "@types/node": "^7.0.43",
        "@types/mocha": "^2.2.42"
    }
}
